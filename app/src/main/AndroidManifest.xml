<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raviphoto">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Table_Activity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".WildLife_Activity"
            android:screenOrientation="portrait"
            />
        <activity android:name=".Wedding_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Child_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Modelling_Activity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"
            />
        <activity
            android:screenOrientation="portrait"
            android:name=".Splash_Screen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>